class Animal:
    def __init__(self,name,age,gender):
        self.name = name
        self.age = age
        self.gender = gender

    def speak(self):
        print("An animal is speaking")

class Cat(Animal):
    def __init__(self,name,age,gender):
        super().__init__(name,age,gender)
        
    def speak(self):
        print("A cat is meawing")

Animala = Animal("Tiger",43,"male")
Animala.speak()

Cata = Cat("Citty",23,"female")
Cata.speak()