x = 20  # int
y= 27.39 # float
z="ravi" # String
isLaxhmanFromIndia = True
print("the value of x is" ,x)
print("type of data stored in x is" , type (x))

print("the value of y is",y)
print("type of data stored in y is" , type (y))

print("the value of z is" ,z)
print("type of data stored in z is" , type (z))

print("the value of isLaxhmanFromIndia" ,isLaxhmanFromIndia)
print("type of data stored in isLaxhmanFromIndia" , type (isLaxhmanFromIndia))

