class Animal:
    def __init__(self,name,age):
        self.name = name
        self.age = age

    def speak(self):
        print("A animal is speaking")

class Dog(Animal):
    def __init__(self,name,age):
       def __init__(self,name,age,gender):
        super().__init__(name,age,gender)

    def speak(self):
        print("A dog is Barking")

Animala = Animal("cow",12)
Animala.speak()

Doga = Dog("Tommy",23)
Doga.speak()
    
    
        