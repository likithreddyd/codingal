dictionary = {
    'a': 10,
    'b': 20,
    'c': 10,
    'd': 30,
    'e': 20,
    'f': 10
}

frequency = {}

for value in dictionary.values():
    if value in frequency:
        frequency[value] += 1
    else:
        frequency[value] = 1

print("Frequency of values:")
for value, count in frequency.items():
    print(f"{value}: {count}")
